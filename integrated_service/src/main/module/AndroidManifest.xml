<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="cn.kc.integrated_service">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application android:name="cn.kc.moduleutils.base.BaseApplication"
            android:allowBackup="true"
            android:icon="@mipmap/log"
            android:label="@string/app_name"
            android:networkSecurityConfig="@xml/network_security_config"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="cn.kc.trainman_apply.dispatchingcommand.view.DispatchingCommandActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

    </application>

</manifest>