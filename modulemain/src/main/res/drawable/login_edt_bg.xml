<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
        android:shape="rectangle">

    <!-- 填充的颜色 -->
    <solid android:color="#6994E0" />

    <!-- 边框的颜色和粗细 -->
    <stroke
            android:width="1dp"
            android:color="#6994E0"
            />

    <!-- android:radius 圆角的半径 -->
    <corners
            android:radius="20dp"
            />
</shape>